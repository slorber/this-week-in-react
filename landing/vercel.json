{
  "github": {
    "silent": true
  },
  "build": {
    "env": {
      "ENABLE_FILE_SYSTEM_API": "1"
    }
  },
  "rewrites": [
    {
      "source": "/issues/:match*",
      "has": [
        {
          "type": "host",
          "value": "revue.thisweekinreact.com"
        }
      ],
      "destination": "https://www.getrevue.co/profile/thisweekinreact/issues/:match*"
    }
  ],
  "redirects": [
    {
      "source": "/:match*",
      "has": [
        {
          "type": "host",
          "value": "revue.thisweekinreact.com"
        }
      ],
      "destination": "https://thisweekinreact.com/:match*",
      "permanent": true
    },
    {
      "source": "/revue",
      "destination": "https://www.getrevue.co/profile/thisweekinreact",
      "permanent": true
    },
    {
      "source": "/archive",
      "destination": "https://www.getrevue.co/profile/thisweekinreact",
      "permanent": true
    },
    {
      "source": "/calendar",
      "destination": "https://docs.google.com/spreadsheets/d/16aQqZV4pJbrm0zvvT9qFm0B1NwLUPQuVFzijt4zSJCk/edit#gid=0",
      "permanent": true
    },
    {
      "source": "/testimonials",
      "destination": "https://twitter.com/sebastienlorber/timelines/1448942785814466561",
      "permanent": true
    },
    {
      "source": "/sponsoring",
      "destination": "https://thisweekinreact.com/sponsor",
      "permanent": true
    },
    {
      "source": "/sponsoring/en",
      "destination": "https://thisweekinreact.com/sponsor",
      "permanent": true
    },
    {
      "source": "/sponsoring/fr",
      "destination": "https://thisweekinreact.com/fr/sponsor",
      "permanent": true
    },
    {
      "source": "/fr/sponsoring",
      "destination": "https://thisweekinreact.com/fr/sponsor",
      "permanent": true
    },
    {
      "source": "/discord",
      "destination": "https://discord.gg/DMy7wRBhrN",
      "permanent": true
    }
  ]
}
