  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "build:vercel": "yarn setup:node && yarn build && yarn embedWebsite",
    "embedWebsite": "yarn workspace website build && cp -R ../website/build/** public",
    "start": "yarn dev",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "setup:node": "remix setup node",
    "postinstall": "yarn setup:node"
  },
  "dependencies": {
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@remix-run/vercel": "^1.1.3",
